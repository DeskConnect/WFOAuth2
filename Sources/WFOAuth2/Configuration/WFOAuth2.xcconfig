PRODUCT_NAME = WFOAuth2
CURRENT_PROJECT_VERSION = 0.1.1
INFOPLIST_FILE = Sources/WFOAuth2/Info.plist
PRODUCT_BUNDLE_IDENTIFIER = com.deskconnect.WFOAuth2
ALWAYS_SEARCH_USER_PATHS = NO
CLANG_ENABLE_OBJC_ARC = YES
CLANG_ENABLE_MODULES = YES
OTHER_CFLAGS = -Wall -Wextra -Wpedantic -Wno-gnu-conditional-omitted-operand

ONLY_ACTIVE_ARCH[config=Debug] = YES
GCC_OPTIMIZATION_LEVEL[config=Debug] = 0
COPY_PHASE_STRIP[config=Debug] = NO
